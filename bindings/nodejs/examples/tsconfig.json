{
    "compilerOptions": {
        "target": "es2019",
        "module": "commonjs",
        "sourceMap": true,
        "strict": true,
        "moduleResolution": "node",
        "types": ["node"],
        "esModuleInterop": true,
        "noImplicitAny": true,
        "removeComments": true,
        "skipLibCheck": true,
        "outDir": "dist"
    }
}
